{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron:dev": "vite --mode=development",
    "electron:build": "vite build && electron-builder",
    "debug:main": "electron --inspect-brk=9229 ./electron/main.cjs",
    "build:win": "vite build && electron-builder --win --x64"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@editorjs/checklist": "^1.6.0",
    "@editorjs/code": "^2.9.2",
    "@editorjs/editorjs": "^2.30.2",
    "@editorjs/header": "^2.8.7",
    "@editorjs/list": "^1.10.0",
    "@editorjs/marker": "^1.4.0",
    "@editorjs/quote": "^2.7.2",
    "@editorjs/simple-image": "^1.6.0",
    "@editorjs/table": "^2.4.2",
    "@editorjs/text-variant-tune": "^1.0.3",
    "ant-design-vue": "4.x",
    "clipboard": "^2.0.11",
    "editorjs-button": "^3.0.3",
    "editorjs-drag-drop": "^1.1.16",
    "editorjs-go": "github:micheleriva/editorjs-go",
    "editorjs-layout": "^1.2.5",
    "moment": "^2.30.1",
    "pinia": "^2.2.0",
    "react-editor-js": "^2.1.0",
    "uuid": "^10.0.0",
    "vue": "^3.4.34",
    "vue-router": "^4.4.0",
    "vue3-json-viewer": "^2.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.12",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.13.1",
    "electron": "31.6.0",
    "electron-builder": "^25.1.7",
    "eslint": "^9.7.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-vue": "^9.27.0",
    "jsdom": "^24.1.1",
    "less": "^4.2.0",
    "npm-run-all2": "^6.2.2",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.4",
    "typescript": "~5.5.4",
    "unplugin-vue-components": "^0.27.3",
    "vite": "^5.3.5",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vitest": "^2.0.4",
    "vue-tsc": "^2.0.29"
  }
}
